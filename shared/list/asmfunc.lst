     9                                  [SECTION .text]   ; オブジェクトファイルではコレを書いてからプログラム記述
    10                                  io_hlt :         ; void io_hlt(void);
    11 00000000 F4                        HLT
    12 00000001 C3                        RET
    13                                  
    14                                  io_cli :
    15 00000002 FA                        CLI
    16 00000003 C3                        RET
    17                                  
    18                                  io_sti :
    19 00000004 FB                        STI
    20 00000005 C3                        RET
    21                                  
    22                                  io_stihlt :
    23 00000006 FB                        STI
