     1                                  [BITS 32]         ; 32bit modeの機械語
     2                                  ; 関数名
     3                                    GLOBAL    io_hlt,io_cli,io_sti,io_stihlt
     4                                    GLOBAL    io_in8, io_in16, io_in32
