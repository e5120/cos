    32                                  
    33                                  io_in16 :
    34 00000014 8B542404                  MOV   EDX,[ESP+4] ;port
    35 00000018 B800000000                MOV   EAX,0
    36 0000001D 66ED                      IN    AX,DX
    37 0000001F C3                        RET
    38                                  
    39                                  io_in32 :
    40 00000020 8B542404                  MOV   EDX,[ESP+4] ;port
    41 00000024 B800000000                MOV   EAX,0
    42 00000029 ED                        IN    EAX,DX
    43 0000002A C3                        RET
    44                                  
    45                                  io_out8 :
    46 0000002B 8B542404                  MOV   EDX,[ESP+4] ;port
    47 0000002F 8A442408                  MOV   AL,[ESP+8]  ;data
    48 00000033 EE                        OUT   DX,AL
    49 00000034 C3                        RET
    50                                  
    51                                  io_out16 :
    52 00000035 8B542404                  MOV   EDX,[ESP+4] ;port
    53 00000039 8B442408                  MOV   EAX,[ESP+8] ;data
    54 0000003D 66EF                      OUT   DX,AX
    55 0000003F C3                        RET
    56                                  
    57                                  io_out32 :
    58 00000040 8B542404                  MOV   EDX,[ESP+4] ;port
    59 00000044 8B442408                  MOV   EAX,[ESP+8] ;data
