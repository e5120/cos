    25 00000008 C3                        RET
    26                                  
    27                                  io_in8 :
    28 00000009 8B542404                  MOV   EDX,[ESP+4]  ;port
    29 0000000D B800000000                MOV   EAX,0
    30 00000012 EC                        IN    AL,DX
